            </intent-filter>
        </activity>
        <activity android:name="com.example.Yang.myapplication.secondActivity"></activity>
        <activity android:name="com.example.Yang.myapplication.thirdActivity"></activity>
        <activity android:name="com.example.Yang.myapplication.forthActivity"></activity>
        <activity android:name="com.example.Yang.myapplication.fifthActivity"></activity>
        <activity android:name="com.example.Yang.myapplication.sixthActivity"></activity>
        <activity android:name="com.example.Yang.myapplication.horizontal"></activity>
        <activity android:name="com.example.Yang.myapplication.left"></activity>
        >
    </application>

</manifest>